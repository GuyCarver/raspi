#! /usr/bin/env 8th
(* module to interface with the pca9865 16 servo controller *)

ns: pca

"pca9865" lib pcalib

"T" "Startup" func: Startup
"T" "IsGood" func: IsGood
"V" "Shutdown" func: Shutdown
"VF" "SetFreq" func: SetFreq
"VU" "Off" func: Off
"V" "AllOff" func: AllOff
"VUF" "Set" func: Set
"VUF" "SetAngle" func: SetAngle

ns: user
